pipeline{
  agent any
    stages{
      stage("build"){
        steps{
          echo "building an application..."
          sh 'sudo apt-get install apache2 -y'
          sh 'sudo systemctl apache2 start'
          sh 'sudo systemctl apache2 enable
        }
      }
      stage("test"){
        steps{
          echo "testing an application..."
         
        }
      }
    }
}
